{
  "manifest_version": 2,

  "name":"Mobilize",
  "version":"1.0",
  "author":"Sammy Shin",
  "description":"Changing the new tab page into a beautiful agenda",

  "chrome_url_overrides" : {
    "newtab": "html/index.html"
  },
  "background": {
    "scripts": ["scripts/main.js", "scripts/jquery-3.2.1.min.js", "scripts/background.js", "scripts/clock.js"],
    "persistent": false
  },
  "permissions": [
    "https://www.unsplash.com/",
    "https://api.unsplash.com/"
  ]
}
