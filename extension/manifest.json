{
  "manifest_version": 2,

  "name":"Mobilize",
  "version":"1.0",
  "author":"Sammy Shin",
  "description":"Changing the new tab page into a beautiful agenda",

  "chrome_url_overrides" : {
    "newtab": "html/index.html"
  },
  "content_scripts": [ {
    "js": [ "jquery-3.2.1.min.js", "main.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
